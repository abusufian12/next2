{
  "name": "@pick-bazar/shop",
  "version": "5.0.1",
  "private": true,
  "scripts": {
    "clean": "rimraf \"{.next,node_modules,__generated__,.cache,src/framework/graphql/**/*.d.ts}\"",
    "gql-clean": "rimraf \"{__generated__,.next,.cache,src/framework/graphql/**/*.d.ts}\"",
    "dev:rest": "next dev -p 3003",
    "build:rest": "next build",
    "codegen": "node -r dotenv/config $(yarn bin)/graphql-let",
    "dev:gql": "yarn codegen && next dev -p 3001",
    "build:gql": "yarn codegen && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "^13.0.6",
    "next-auth": "4.3.1",
    "next-i18next": "10.5.0",
    "next-pwa": "5.4.6",
    "next-seo": "5.1.0",
    "react": "^18.2.0",
    "react-content-loader": "6.2.0",
    "react-copy-to-clipboard": "5.0.4",
    "react-device-detect": "2.1.2",
    "react-dom": "^18.2.0",
    "react-dropzone": "12.0.4",
    "react-hook-form": "7.28.1",
    "react-otp-input": "2.4.0",
    "react-phone-input-2": "2.15.0",
    "react-query": "3.34.16",
    "react-scroll": "1.8.6",
    "react-select": "5.2.2",
    "react-sticky-box": "1.0.1",
    "react-toastify": "8.2.0",
    "react-use": "17.3.2",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@types/overlayscrollbars": "1.12.1",
    "@types/react": "17.0.41",
    "@types/react-copy-to-clipboard": "5.0.2",
    "@types/react-mailchimp-subscribe": "2.1.1",
    "@types/react-scroll": "1.8.3",
    "@types/react-select": "5.0.1",
    "husky": ">=6",
    "typescript": "4.9.4",
    "yaml-loader": "0.6.0"
  }
}
